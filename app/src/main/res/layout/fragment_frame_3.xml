<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frame_3"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:backgroundTint="@color/colorOffwhite"
    android:visibility="visible">
    <View
        android:id="@drawable/ic_bubbles_1"
        android:layout_width="414dp"
        android:layout_height="872dp"
        android:background="@drawable/ic_bubbles_1"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="520dp"
        android:layout_height="451dp"
        android:layout_marginBottom="432dp"
        android:src="@drawable/ic_launcher_background"
        android:visibility="visible"
        android:elevation="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="400dp"
        android:layout_height="350dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75">


        <View
            android:id="@+id/ic_card"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/ic_card"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/res_primary"
            android:layout_width="237dp"
            android:layout_height="68dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="71dp"
            android:layout_marginTop="86dp"
            android:layout_marginEnd="92dp"
            android:layout_marginBottom="196dp"
            android:fontFamily="@font/red_hat_display_black"
            android:gravity="top"
            android:text="@string/res_primary"
            android:textAppearance="@style/res_primary" />

        <TextView
            android:id="@+id/res_secondary"
            style="@style/res_secondary"
            android:layout_width="204dp"
            android:layout_height="48dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="72dp"
            android:layout_marginTop="197dp"
            android:layout_marginEnd="124dp"
            android:layout_marginBottom="105dp"
            android:fontFamily="@font/red_hat_display_black"
            android:text="@string/res_secondary" />

        <TextView
            android:id="@+id/conf_primary"
            style="@style/conf_primary"
            android:layout_width="144dp"
            android:layout_height="34dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="72dp"
            android:layout_marginTop="152dp"
            android:layout_marginEnd="184dp"
            android:layout_marginBottom="164dp"
            android:fontFamily="@font/red_hat_display_medium"
            android:text="@string/conf_primary" />

        <TextView
            android:id="@+id/conf_secondary"
            style="@style/conf_secondary"
            android:layout_width="53dp"
            android:layout_height="32dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="72dp"
            android:layout_marginEnd="166dp"
            android:layout_marginBottom="74dp"
            android:fontFamily="@font/red_hat_display_medium"
            android:text="@string/conf_secondary" />

        <TextView
            android:id="@+id/result_heading"
            android:layout_width="160dp"
            android:layout_height="33dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="70dp"
            android:layout_marginTop="47dp"
            android:layout_marginEnd="170dp"
            android:layout_marginBottom="270dp"
            android:fontFamily="@font/red_hat_display_medium"
            android:gravity="top"
            android:text="@string/results_heading"
            android:textAppearance="@style/results_heading" />

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </RelativeLayout>

    <Button
        android:id="@+id/save_button"
        android:layout_width="189dp"
        android:layout_height="56dp"
        android:background="@drawable/rectangle_1"
        android:elevation="35dp"
        android:fontFamily="@font/red_hat_display_black"
        android:text="Save"
        android:textColor="@color/colorOffwhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.84000003" />

</androidx.constraintlayout.widget.ConstraintLayout>
